{
	"expo": {
		"name": "transporter",
		"slug": "transporter-expo",
		"privacy": "public",
		"sdkVersion": "32.0.0",
		"platforms": [
			"ios",
			"android"
		],
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"updates": {
			"fallbackToCacheTimeout": 30000,
			"enabled" : true,
			"checkAutomatically" : "ON_LOAD"
		},
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "cover"
		},
		"assetBundlePatterns": [
			"**/*"
		],
		"ios": {
			"bundleIdentifier": "com.transporter.app",
			"supportsTablet": true
		},
		"android": {
			"package": "com.transporter.app",
			"permissions": [
				"CAMERA",
				"READ_EXTERNAL_STORAGE",
				"WRITE_EXTERNAL_STORAGE",
				"LOCATION",
				"ACCESS_COARSE_LOCATION",
				"ACCESS_FINE_LOCATION"
			],
			"config": {
				"googleMaps": {
					"apiKey": "AIzaSyDu9JLR_CCvfdyuaYnkkkfFqJD8e1_Ro28"
				}
			}
		},
		"description": ""
	}
}